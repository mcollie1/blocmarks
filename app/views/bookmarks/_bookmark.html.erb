<div class="row">
  <div class="col-md-20 col-md-2">
    <% bookmark.urls.each do |url| %>
      <% if current_user.votes.where(url_id: url.id) %>
        <% object = LinkThumbnailer.generate(url.url, redirect_limit: 25) %>
        <%= object.title %>

        <% if object.url? %>
          <%= link_to(image_tag(object.image, size: "100x100"), object.url ) %>
        <% else %>
          <%= link_to(image_tag("icon.png", size: "100x100"), object.url ) %>
        <% end %>
      <% else %>
        <% nil %>
      <% end %>
    <% end %>
  </div>
</div>

